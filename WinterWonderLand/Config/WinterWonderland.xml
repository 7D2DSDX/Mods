	<config name="biomes">
		<append xpath="/worldgeneration/biomes/biome[@name='snow']/decorations">	
			<decoration type="block" blockname="treeWinterEverGreen" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="BrokenTree3Prefab" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="BrokenTree2Prefab" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="BrokenTreePrefab" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="PineTreePrefab" prob="0.0001" rotatemax="7" />
			<decoration type="block" blockname="PineTree2Prefab" prob="0.001" rotatemax="7" />
			<decoration type="block" blockname="Tree5Prefab" prob="0.001" />
			<decoration type="block" blockname="Tree4Prefab" prob="0.001" />
			<decoration type="block" blockname="Tree3Prefab" prob="0.001" />
			<decoration type="block" blockname="Tree2Prefab" prob="0.001" />
